{
   "action": {
      "default_icon": {
         "128": "img/icon/icon128.png",
         "16": "img/icon/icon16.png",
         "48": "img/icon/icon48.png",
         "64": "img/icon/icon64.png"
      },
      "default_popup": "popup.html",
      "default_title": "__MSG_appName__"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "lib/js/browser-polyfill.js", "content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'; worker-src 'self';"
   },
   "default_locale": "pl",
   "description": "__MSG_appDesc__",
   "host_permissions": [ "*://*.buykers.at/", "*://*.buykers.com.au/", "*://*.buykers.cz/", "*://*.buykers.de/", "*://*.buykers.co.uk/", "*://*.buykers.es/", "*://*.buykers.nl/", "*://*.buykers.com.tr/", "*://*.buykers.com/", "*://*.alerabat.com/", "*://*.alerabat.local/" ],
   "icons": {
      "128": "img/icon/icon128.png",
      "16": "img/icon/icon16.png",
      "48": "img/icon/icon48.png",
      "64": "img/icon/icon64.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_appName__",
   "permissions": [ "activeTab", "webNavigation", "cookies", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.28.2",
   "web_accessible_resources": [ {
      "matches": [ "*://*/*" ],
      "resources": [ "img/*.png", "img/*.svg", "content.css", "fonts/*.woff2" ]
   } ]
}
