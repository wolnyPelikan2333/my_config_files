{
   "action": {
      "default_icon": "icons/icon19.png",
      "default_title": "Just Read"
   },
   "background": {
      "service_worker": "background.js"
   },
   "browser_specific_settings": {
      "gecko": {
         "id": "{cb2b337b-99d7-4b86-aa04-84a6f5c3e218}"
      }
   },
   "commands": {
      "open-just-read": {
         "description": "Open the Just Read format for the current page",
         "suggested_key": {
            "default": "Ctrl+Shift+L"
         }
      },
      "select-text": {
         "description": "Enable user text selection mode using Just Read",
         "suggested_key": {
            "default": "Ctrl+Shift+K"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "messager.js" ],
      "matches": [ "https://justread.link/*" ]
   } ],
   "description": "A feature-packed, customizable reader extension.",
   "homepage_url": "https://github.com/ZachSaucier/Just-Read",
   "host_permissions": [ "*://*/*" ],
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Just Read",
   "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
   },
   "permissions": [ "tabs", "contextMenus", "storage", "scripting" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "5.3.4",
   "web_accessible_resources": [ {
      "matches": [ "*://*/*" ],
      "resources": [ "print.svg", "required-styles.css", "default-styles.css", "dark-styles.css", "hide-segments.css", "page.css", "options.js", "options.css", "fonts/*" ]
   } ]
}
